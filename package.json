{
  "name": "trendly-brands",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --port 8082",
    "android": "expo run:android",
    "ios": "expo run:ios --port 8082",
    "ios-device": "expo run:ios --device --port 8082",
    "web": "expo start --web",
    "eas-build-pre-install": "./eas-prebuild.sh",
    "test": "jest --watchAll",
    "build": "expo export -p web",
    "preview": "export $(cat .env.local | xargs) &&  eas build --platform ios --profile preview --local --output build/Trendly.tar.gz && tar -xzf build/Trendly.tar.gz -C build && xcrun simctl install booted build/TrendlyBrands.app",
    "simulator": "export $(cat .env.local | xargs) &&  eas build --platform ios --profile simulator --local --output build/Trendly.tar.gz && tar -xzf build/Trendly.tar.gz -C build && xcrun simctl install booted build/TrendlyBrands.app",
    "emulator": "export $(cat .env.local | xargs) &&  eas build --platform android --profile simulator --local --output build/Trendly.apk && adb install build/Trendly.apk",
    "build-ios": "export $(cat .env.local | xargs) &&  eas build --platform ios --profile production --local --output build/Trendly.ipa",
    "build-android": "export $(cat .env.local | xargs) &&  eas build --platform android --profile production --local --output build/Trendly.aab",
    "build-android-apk": "export $(cat .env.local | xargs) && eas build --platform android --profile production-apk --local --output build/Trendly.apk",
    "submit-ios": "eas submit -p ios --path build/Trendly.ipa",
    "submit-android": "aws s3 cp build/Trendly.apk s3://trendly-assets/trendly-brand.apk --acl public-read",
    "postinstall": "node react-native-web-fix.js"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
